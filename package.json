  "repository": {
    "type": "git",
    "url": "https://github.com/bibixx/frontend-boilerplate",
    "ghbranch": "gh-pages"
  },
  "config": {
    "title": "Your app name"
  },
  "browserslist": [
    "extends browserslist-config-google"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.js",
    "deploy": "sh ./build/deploy.sh",
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js -p --progress"
  },
  "dependencies": {
    "history": "^4.7.2",
    "raw-loader": "^0.5.1",
